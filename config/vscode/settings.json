{
    "gitlab.debug": true,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "Catppuccin Mocha",
    "editor.lineNumbers": "on",
    "vscode-neovim.useCtrlKeysForNormalMode": false,
    "vscode-neovim.useCtrlKeysForInsertMode": false,
    "keyboard.dispatch": "keyCode",
    "workbench.activityBar.location": "default",
    "workbench.statusBar.visible": false,
    "breadcrumbs.enabled": false,
    "window.menuBarVisibility": "toggle",
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.horizontal": "hidden",
    "editor.overviewRulerBorder": false,
    "editor.hideCursorInOverviewRuler": true,
    "workbench.layoutControl.enabled": false,
    "editor.padding.top": 8,
    "editor.padding.bottom": 8,
    "github.copilot.enable": {
        "*": false
    },
    "workbench.activityBar.iconClickBehavior": "toggle",
    "workbench.accounts.visible": false,
    "extensions.ignoreRecommendations": true,
    "workbench.commandPalette.experimental.askChatLocation": "hidden",
    "chat.commandCenter.enabled": false,
    "workbench.sideBar.location": "left",
    "workbench.panel.defaultLocation": "bottom",
    "http.proxyStrictSSL": false
}
